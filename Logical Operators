<body>
    <div class="container">
        <h1>Logical Operator</h1>
        <?php
        
        //Logical and(&&)
        $variable1=(True) and (True);
        echo var_dump($variable1);
        echo"<br>";
       
        $variable1=(False) && (True);
        echo var_dump($variable1);
        echo"<br>";

        //Logical or(||)
        $variable1=(True) or (True);
        echo var_dump($variable1);
        echo"<br>";

        $variable1=(False) || (True);
        echo var_dump($variable1);
        echo"<br>";

        //Logical xor  Both same is false
        $variable1=(False xor True);
        echo var_dump($variable1);
        echo"<br>";

        $variable1=(False xor false);
        echo var_dump($variable1);
        echo"<br>";

        $variable1=(true xor False);
        echo var_dump($variable1);
        echo"<br>";

        $variable1=(True xor True);
        echo var_dump($variable1);
        echo"<br>";

        ?>
    </div>
</body>
</html>
